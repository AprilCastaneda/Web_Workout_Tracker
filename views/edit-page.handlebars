<h1>Edit Exercise:</h1>

<p>{{results}}</p>

<div class="formContainer">
        <form>
                <fieldset>
                        <legend>Edit Row</legend>
                                <div>
                                        <label for="name">Name:</label>
                                        <input type="text" name="name" id="editName">
                                </div>
                                <div>
                                        <label for="reps">Reps (#):</label>
                                        <input type="number" name="reps" id="editReps">
                                </div>
                                <div>
                                        <label for="weight">Weight (#):</label>
                                        <input type="number" name="weight" id="editWeight">
                                </div>
                                <div>
                                        <label for="date">Date:</label>
                                        <input type="date" name="date" id="editDate">
                                </div>
                                <div>
                                        <label for="unit">Unit:</label>
                                        <input type="radio" name="lbs" id="editUnitLbs" value="1">lbs
                                        <input type="radio" name="lbs" id="editUnitKgs" value="0">kgs
                                </div>
                                <div>
                                        <input type="submit" name="submit" id="editExercise">
                                </div>
                </fieldset> 
        </form>
</div>
<div id="editDisplay"></div>
